<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ecs160.homestaymanager.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ecs160.homestaymanager.app.MainActivity"
            android:label="Main Acttvity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.ecs160.homestaymanager.app.RegisterProfile"
            android:label="@string/title_activity_reg_profile" >
        </activity>
        <activity
            android:name="com.ecs160.homestaymanager.app.EditProfile"
            android:label="@string/title_activity_reg_profile" >
        </activity>

    </application>

</manifest>
